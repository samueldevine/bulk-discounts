<h1>Update your bulk discount</h1>

<%= form_with model: @bulk_discount, url: merchant_bulk_discount_path(@merchant, @bulk_discount), local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>

  <%= f.label :percentage %>
  <%= f.text_field :percentage %><br>

  <%= f.label :quantity %>
  <%= f.text_field :quantity %><br>

  <%= f.submit %>
<% end %>
